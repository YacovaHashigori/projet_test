{% extends "base.html.twig" %} {% block body %} {% for chateau in chateaux %}
<div>
  <p>L'id du chateau est : {{ chateau.id }}</p>
  <p>{{ chateau.name }}</p>
  <p>Description du chateau : {{ chateau.description }}</p>
  <a href="{{ path('chateau_readone', { id: chateau.id }) }}">Voir le chateau</a>
</div>
{% endfor %} {% endblock %}
